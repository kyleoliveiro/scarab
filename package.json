{
  "name": "scarab-scss",
  "version": "5.0.1",
  "description": "Sass utility framework for rapid stylesheet development",
  "keywords": [
    "sass",
    "scss",
    "mixins",
    "responsive",
    "front-end"
  ],
  "main": "scss/scarab.scss",
  "scripts": {
    "build": "node-sass --error-bell _.scss -o build/",
    "start": "onchange \"scss/**/*.scss\" -- npm run build"
  },
