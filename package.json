  "repository": {
    "type": "git",
    "url": "https://github.com/watchtowerdigital/scarab"
  },
  "dependencies": {
    "node-sass": "^3.14.0-0",
    "onchange": "^3.2.0"
  },
  "devDependencies": {}
}
